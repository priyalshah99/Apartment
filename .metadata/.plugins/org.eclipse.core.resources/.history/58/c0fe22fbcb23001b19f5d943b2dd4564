package com.apartment.response;

import com.apartment.models.Owner;

public class OwnerResponse {
	
	private String name;
	private String contactNo;
	private String email;
	private String dateOfPurchase;
	
	public OwnerResponse(String name, String contactNo, String email, String dateOfPurchase) {
		super();
		this.name = name;
		this.contactNo = contactNo;
		this.email = email;
		this.dateOfPurchase = dateOfPurchase;
	}

	public static MeetingResponse build(Owner owner) {
		return new OwnerResponse(owner.getName(), owner.getContactNo(), owner.getEmail(), owner.getDateOfPurchase());
	}
	
	
	
	
}
